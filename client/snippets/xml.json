{
    "i18n": {
        "prefix": "i18n",
        "description": "Creates a model reference with i18n address",
        "body": "\"{i18n>$0}\""
    },
    "i18nlabel": {
        "prefix": "i18nlabel",
        "description": "Creates a label with i18n address",
        "body": "<Label text=\"{i18n>$0}\"/>"
    },
    "i18ntitle": {
        "prefix": "i18ntitle",
        "description": "Creates a title with i18n address",
        "body": "<Title text=\"{i18n>$0}\"/>"
    },
    "model": {
        "prefix": "sapmodel",
        "description": "Creates a sap.m.Text element with text property",
        "body": "{${1:modelname}>/$0}"
    },
    "XMLView": {
        "description": "Inserts a new UI5 XML view",
        "prefix": "ui5xmlview",
        "body": ["<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"yes\" ?>",
            "<mvc:View",
            "    xmlns=\"sap.m\"",
            "    xmlns:l=\"sap.ui.layout\"",
            "    xmlns:mvc=\"sap.ui.core.mvc\"",
            "    controllerName=\"$1\">",
            "\t$0",
            "</mvc:View>"
        ]
    }
}