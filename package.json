{
    "name": "openui5ts",
    "displayName": "openui5 Typescript Tool",
    "description": "A typescript extension for openui5",
    "version": "0.0.1",
    "publisher": "apazureck",
    "engines": {
        "vscode": "^1.5.0"
    },
    "author": {
        "name": "Andreas Pazureck",
        "email": "andreas@pazureck.de",
        "url": "www.pazureck.de"
    },
    "icon": "./img/OpenUI5_logo.png",
    "categories": [
        "Productivity"
    ],
    "activationEvents": [
        "onCommand:extension.SetupUi5",
        "workspaceContains:manifest.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "jsonValidation": [{
            "fileMatch": "manifest.json",
            "url": "./src/jsonschemas/manifest.schema.json"
            }
        ],
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/typescript.json."
            }
        ],
        "commands": [
            {
                "category": "UI5",
                "command": "extension.SetupUi5",
                "title": "UI5 SetupProject"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}